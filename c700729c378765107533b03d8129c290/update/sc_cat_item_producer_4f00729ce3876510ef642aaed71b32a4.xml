        <script><![CDATA[/** This script is executed before the Record is generated
 * `current`- GlideRecord produced by Record Producer
 * Don't use `current.update()` or `current.insert()` as the record is generated by Record Producer
 * Don't use `current.setValue('sys_class_name', 'xxx')` as this will trigger reparent flow and can cause data loss
 * Avoid `current.setAbortAction()` and generate a separate record
 * Use `producer.var1` to access variables
 */

var currency = new GlideRecord("fx_currency_test");
currency.initialize();
var userSessionCode = currency.amount.getSessionCurrencyCode();

var totalCost = 0;
var comments = "Expense Line items"+"\n";
for (var i = 0; i < producer.expense_line_items.getRowCount(); i++) {
    generateExpenseLines(producer.expense_line_items.getRow(i)["expense_name"], producer.expense_line_items.getRow(i)["expense_amount"]);
	comments += "\n"+producer.expense_line_items.getRow(i)["expense_name"] +" : "+producer.expense_line_items.getRow(i)["expense_amount"] + " "+producer.expense_line_items.getRow(i)["currency"];
}

function generateExpenseLines(name, amount) {
    var expLines = new GlideRecord("x_snc_ow_credit_0_expense_line_item");
    expLines.initialize();
    expLines.expense_request = current.sys_id;
    expLines.expense_name = name;
    expLines.expense_amount.setDisplayValue(userSessionCode + ";" + amount);
    expLines.insert();
    totalCost += parseFloat(expLines.expense_amount.amount);
}
current.comments = comments;
current.total_cost.setDisplayValue(userSessionCode + ';' + totalCost);]]></script>
